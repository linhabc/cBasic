#include "linkedAddr.h"

int main(){
  int n;

  do{
    printf("MENU");
    printf("1. \n");
    printf("2. \n");
    printf("3. \n");
    printf("4. \n");
    printf("5. \n");
    printf("6. \n");
    do{
      printf("Enter: ");
      scanf(" %d", &n);
    }while(n> || n<1);




    
  }while();
    
  return 0;
}
